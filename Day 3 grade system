#include<stdio.h>
int main(){
    int grade;
    printf("Enter the grade");
    scanf("%d",&grade);
    if(grade>85){
        printf("A");
    }else if(grade>70){
        printf("B");
    }else if(grade>60){
        printf("c");
    }else if(grade>50){
        printf("D");
    }else{
        printf("e");
        return 0;
    }
    }
